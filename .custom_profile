last_output() {
     if [ $? -eq 0 ]; then
          printf "\xE2\x9C\x85"
     else
          printf "\xE2\x9D\x8C"
     fi
}
parse_git_branch() {
     git branch 2> /dev/null | sed -e '/^[^*]/d' -e 's/* \(.*\)/ (\1)/'
}
PROMPT_COMMAND=last_output
PS1=" \u \[\033[32m\]\w\[\033[33m\]\$(parse_git_branch)\[\033[00m\] \t \n\$ "
